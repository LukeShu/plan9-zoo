config_interface: textconfig
display_library: x,options="gui_debug"
#display_library: x,options="gui_debug"

memory: guest=32, host=32, block_size=128
romimage: file="/usr/share/bochs/BIOS-bochs-latest", address=0x00000000, options=none, flash_data=none
vgaromimage: file="/usr/share/bochs/VGABIOS-lgpl-latest.bin"

boot: disk
floppy_bootsig_check: disabled=0
floppya: type=1_44, 1_44="/home/lukeshu/Projects/Plan9/plan9-run-historical/plan9-1e/sys/lib/pcdisk", status=inserted, write_protected=1
# no floppyb
ata0: enabled=true, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=disk, path="/home/lukeshu/Projects/Plan9/plan9-run-historical/plan9-1e-fdboot.img", mode=flat, cylinders=0, heads=16, spt=63, sect_size=512, model="Generic 1234", biosdetect=auto, translation=auto
ata0-slave: type=none
ata1: enabled=true, ioaddr1=0x170, ioaddr2=0x370, irq=15
ata1-master: type=none
ata1-slave: type=none
ata2: enabled=false
ata3: enabled=false
optromimage1: file=none
optromimage2: file=none
optromimage3: file=none
optromimage4: file=none
optramimage1: file=none
optramimage2: file=none
optramimage3: file=none
optramimage4: file=none
pci: enabled=1, chipset=i440fx, slot1=none, slot2=none, slot3=none, slot4=none, slot5=none
vga: extension=vbe, update_freq=10, realtime=1, ddc=builtin, vbe_memsize=16
cpu: count=1, ips=4000000, model=i486dx4, reset_on_triple_fault=0, cpuid_limit_winnt=0, ignore_bad_msrs=1
print_timestamps: enabled=0
debugger_log: -
magic_break: enabled=1 0x8
port_e9_hack: enabled=false, all_rings=false
iodebug: all_rings=0
private_colormap: enabled=0
clock: sync=realtime, time0=local, rtc_sync=0
# no cmosimage
log: -
logprefix: %t%e%d
debug: action=ignore
info: action=report
error: action=report
panic: action=ask, cpu0=fatal, siminterface=fatal
keyboard: type=mf, serial_delay=150, paste_delay=100000, user_shortcut=none
mouse: type=ps2, enabled=false, toggle=ctrl+mbutton
